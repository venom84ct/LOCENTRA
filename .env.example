# Example environment configuration for LOCENTRA

# Base path for production builds when deploying under a subdirectory
# Leave empty for root deployment
VITE_BASE_PATH=

# URL of your Supabase project
VITE_SUPABASE_URL=

# Public anon key from Supabase project settings
VITE_SUPABASE_ANON_KEY=

# Toggle Tempo dev tools in the UI ("true" or "false")
VITE_TEMPO=

# OneSignal Web Push App ID for client initialisation
VITE_ONESIGNAL_APP_ID=

# REST API key used by the send_push_notification edge function
ONESIGNAL_REST_API_KEY=

# Secret key for authenticating with the Pica API
PICA_SECRET_KEY=

# Stripe connection key used with Pica payments
PICA_STRIPE_CONNECTION_KEY=

# Service role key used by edge functions such as reset_leaderboard
SUPABASE_SERVICE_ROLE_KEY=

# Supabase URL used by edge functions
SUPABASE_URL=

# Project ID used when generating Supabase types
SUPABASE_PROJECT_ID=
